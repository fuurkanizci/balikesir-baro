+if (document.querySelector('.counting')) {
+    // Intersection Observer options
+    const options = {
+        root: null, // viewport
+        rootMargin: '0px',
+        threshold: 0.5 // %50'si görünür olduğunda çalışsın
+    };
+
+    // Observer callback fonksiyonu
+    const handleIntersect = (entries, observer) => {
+        entries.forEach(entry => {
+            // Eğer element görünür olduysa
+            if (entry.isIntersecting) {
+                const el = entry.target; // Görünen element
+                let countTo = parseInt(el.getAttribute('data-count'));
+                let countNum = parseInt(el.textContent);
+
+                // Number format için ayar
+                const numberFormat = new Intl.NumberFormat('tr-TR'); // Türkçe format
+
+                // Para formatı için ayrı ayar (ilk eleman için)
+                const currencyFormat = new Intl.NumberFormat('tr-TR', {
+                    style: 'currency',
+                    currency: 'TRY',
+                });
+
+                let startTime = null;
+
+                // Sayacın animasyonu
+                function animateCount(currentTime) {
+                    if (!startTime) startTime = currentTime;
+                    let elapsedTime = currentTime - startTime;
+                    let duration = 4000; // 4 saniye
+                    let progress = Math.min(elapsedTime / duration, 1);
+                    let currentCount = Math.floor(countNum + progress * (countTo - countNum));
+
+                    // Eğer ilk eleman (para formatı)
+                    if (el.classList.contains('first-counting')) {
+                        el.textContent = currencyFormat.format(currentCount); // Para formatı
+                    } else {
+                        el.textContent = numberFormat.format(currentCount); // Sayısal format
+                    }
+
+                    if (progress < 1) {
+                        requestAnimationFrame(animateCount);
+                    } else {
+                        // Animasyon bittiğinde
+                        if (el.classList.contains('first-counting')) {
+                            el.textContent = currencyFormat.format(countTo); // Para formatı
+                        } else {
+                            el.textContent = numberFormat.format(countTo); // Sayısal format
+                        }
+                    }
+                }
+
+                // Sayacın animasyonunu başlat
+                requestAnimationFrame(animateCount);
+
+                // Observer'ı durdur, animasyon başladıktan sonra
+                observer.unobserve(el);
+            }
+        });
+    };
+
+    // Observer'ı başlat
+    const observer = new IntersectionObserver(handleIntersect, options);
+
+    // Tüm .counting öğelerini gözlemlemeye al
+    document.querySelectorAll('.counting').forEach(el => {
+        observer.observe(el);
+    });
+}
